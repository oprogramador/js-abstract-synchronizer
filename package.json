{
  "name": "js-abstract-synchronizer",
  "version": "3.0.0",
  "description": "abstract synchronizer for backend and frontend",
  "license": "MIT",
  "main": "./build/index.js",
  "keywords": [
    "abstract",
    "synchronize",
    "synchronizer",
    "serialize",
    "serializer",
    "orm",
    "odm",
    "data"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oprogramador/js-abstract-synchronizer.git"
  },
  "scripts": {
    "build": "rm -rf build && babel app --out-dir build --ignore tests/",
    "create-project-symlink": "scripts/create-project-symlink.sh",
    "lint": "eslint app",
    "postinstall": "npm run create-project-symlink",
    "prepublishOnly": "npm run build",
    "start": "scripts/wait_for_db.sh && npm test",
    "self-install": "scripts/self_install.sh",
    "test-mocha": "mocha -r babel-register --recursive app/tests --timeout 5000",
    "test": "npm run self-install && npm run lint && npm run test-mocha"
  },
  "dependencies": {
    "arangodb-error-codes": "^1.0.8",
    "arangojs": "^6.10.0",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "cors": "^2.8.5",
    "es6-error": "^4.1.1",
    "express": "^4.16.4",
    "express-bunyan-logger": "^1.3.3",
    "http-status": "^1.3.1",
    "json-schema-filter": "^0.1.5",
    "jsonschema": "^1.2.4",
    "lodash": "^4.17.11",
    "superagent": "^4.1.0",
    "superagent-bluebird-promise": "^4.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-improved": "^4.0.5",
    "faker": "^4.1.0",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "sinon": "^7.3.1",
    "sinon-chai": "^3.3.0",
    "supertest": "^4.0.2",
    "supertest-as-promised": "^4.0.2"
  }
}
